<p id="notice"><%= notice %></p>

<h1>Oauth Services</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @oauth_services.each do |oauth_service| %>
      <tr>
        <td><%= link_to 'Show', oauth_service %></td>
        <td><%= link_to 'Edit', edit_oauth_service_path(oauth_service) %></td>
        <td><%= link_to 'Destroy', oauth_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Oauth Service', new_oauth_service_path %>
